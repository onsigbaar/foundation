{
  "name"        : "onsigbaar/foundation",
  "type"        : "library",
  "description" : "Foundation Onsigbaar",
  "keywords"    : [
    "Onsigbaar",
    "Foundation"
  ],
  "homepage"    : "https://github.com/onsigbaar/foundation",
  "license"     : "MIT",
  "authors"     : [
    {
      "name"    : "anonymoussc",
      "email"   : "50c5ac69@opayq.com",
      "homepage": "https://anonymoussc.github.io/",
      "role"    : "Developer"
    }
  ],
  "require"     : {
    "php"                      : "~5.6|~7.0",
    "illuminate/support"       : "5.5.*",
    "illuminate/contracts"     : "5.5.*",
    "onsigbaar/components"     : "0.4.*",
    "onsigbaar/modules"        : "0.1.*",
    "jeroen-g/laravel-packager": "^1.5"
  },
  "require-dev" : {
    "phpunit/phpunit"          : "~4.0||~5.0",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload"    : {
    "psr-4": {
      "Onsigbaar\\Foundation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Onsigbaar\\Foundation\\": "tests"
    }
  },
  "scripts"     : {
    "test"       : "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style"  : "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra"       : {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config"      : {
    "sort-packages": true
  }
}
